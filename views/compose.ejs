<%- include("partials/header") -%>

<div class="container">
  <h1>Compose</h1>
  <h4><strong>Title</strong></h4>

  <form action="/compose" method="post">
    <div>
      <input class="title" type="text" name="postTitle" />
      <h4><strong>Post</strong></h4>
      <textarea name="postStory" id="" cols="155" rows="6"></textarea>

      <button class="btn btn-primary" type="submit" name="publish">
        Publish
      </button>
    </div>
  </form>
</div>
<%- include("partials/footer") -%>
